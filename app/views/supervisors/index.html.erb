


<div class="row">
  <div class="col-sm-6">
    <h1>Supervisors</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_supervisor_path, class: 'btn btn-primary' do %>
    Add New Supervisor
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>Nombre</th>
            <th>Rut</th>
            <th>Cargo</th>
            <th>Año ingreso</th>
            <th>Otros</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @supervisors.each do |supervisor| %>
        <%= content_tag :tr, id: dom_id(supervisor), class: dom_class(supervisor) do %>
          

                      <td><%= supervisor.nombre %></td>
                      <td><%= supervisor.rut %></td>
                      <td><%= supervisor.cargo %></td>
                      <td><%= supervisor.año_ingreso %></td>
                      <td><%= supervisor.otros %></td>
          
          
            <td><%= link_to 'Show', supervisor %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
