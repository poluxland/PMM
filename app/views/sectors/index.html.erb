


<div class="row">
  <div class="col-sm-6">
    <h1>Sectors</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_sector_path, class: 'btn btn-primary' do %>
    Add New Sector
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>Area</th>
            <th>Instalacion</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @sectors.each do |sector| %>
        <%= content_tag :tr, id: dom_id(sector), class: dom_class(sector) do %>
          

                      <td><%= sector.area %></td>
                      <td><%= sector.instalacion %></td>
          
          
            <td><%= link_to 'Show', sector %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
